generators := func(E,G);
    $ select the groups generated by the elements of maximal order
    local groups,i;
    i := id(E,G);
    groups := {};
    os := { [O(k),E(k)] : k in [1..#E]}
          where O := orders(E,G,i) end;
    E := {x : x in E};
    m := %max domain(os);
    while #E /= 0 do
        g := arb( os{m} );

    end while;
    return groups;
end func;

